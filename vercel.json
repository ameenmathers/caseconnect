{
    "version": 2,
    "framework": null,
    "outputDirectory": "public",
    "buildCommand": "npm run build",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "CaseConnect",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://caseconnect.vercel.app",
        "DB_CONNECTION": "sqlite",
        "ASSEMBLYAI_API_KEY": "a1b88dc1462f4491bdb31786e17ebbda"
    }
}
